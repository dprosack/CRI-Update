685011535],\r\n          zoom: 9,\r\n          spatialReference: {\r\n            wkid: 3857\r\n          }\r\n        });\r\n\r\n        const gLayer = new GraphicsLayer();\r\n\r\n        const sketchModel = new SketchViewModel({\r\n          view: mapView,\r\n          layer: gLayer,\r\n          polylineSymbol: {\r\n            type: \"simple-line\",\r\n            color: [204, 0, 0],\r\n            width: 2,\r\n            style: \"dash\"\r\n          }\r\n        });\r\n\r\n        const sketch = new Sketch({\r\n          view: mapView,\r\n          layer: gLayer,\r\n          viewModel: sketchModel\r\n        });\r\n        \r\n        sketch.create(\"polyline\",{mode:\"click\", hasZ: false})\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components/Map"}]}