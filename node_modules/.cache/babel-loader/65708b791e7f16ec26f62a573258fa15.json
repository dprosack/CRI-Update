,CAAA,KAAA,CAAA,GAAA;AACA,cAAA,WAAA;AACA,cAAA,cAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA,GA3BA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AAAA;;AACA,MAAA,UAAA,GAAA,IAAA,CAAA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,IAAA,UAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,CAAA,CAAA;AACA,OAFA;AAGA;AALA,GA5BA;AAoCA,EAAA,KAAA,EAAA;AACA,IAAA,aADA,2BACA;AAAA;;AACA,MAAA,UAAA,GAAA,IAAA,CAAA,UAAA,MAAA;AAAA,eACA,MAAA,CAAA,aAAA,IAAA,UAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,CAAA,CADA;AAAA,OAAA;AAGA,KALA;AAOA,IAAA,UAAA,EAAA;AACA,MAAA,OAAA,EAAA,mBAAA;AAAA;;AACA,QAAA,aAAA,GAAA,IAAA,CAAA,UAAA,MAAA;AAAA,iBACA,MAAA,CAAA,UAAA,IAAA,UAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,CAAA,CADA;AAAA,SAAA;AAEA,OAJA;AAKA,MAAA,SAAA,EAAA;AALA,KAPA;AAeA,IAAA,YAAA,EAAA;AACA,MAAA,OAAA,EAAA,mBAAA;AAAA;;AACA,QAAA,YAAA,GAAA,IAAA,CAAA,UAAA,MAAA;AAAA,iBACA,MAAA,CAAA,YAAA,IAAA,UAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,CAAA,CADA;AAAA,SAAA;AAEA,OAJA;AAKA,MAAA,SAAA,EAAA;AALA;AAfA,GApCA;AA4DA,EAAA,QAAA,EAAA;AACA,IAAA,UAAA,EAAA,sBAAA;AACA,aAAA,KAAA,WAAA,GAAA,KAAA,aAAA,GAAA,KAAA,YAAA,GAAA,KAAA,UAAA;AACA,KAHA;AAKA,IAAA,YAAA,EAAA,wBAAA;AACA,aAAA,KAAA,aAAA,GAAA,KAAA,YAAA,GAAA,KAAA,UAAA;AACA;AAPA;AA5DA,CAAA","sourcesContent":["<template>\r\n    <div id=\"viewDiv\" class=\"main\">\r\n        <mileageReport />\r\n        <div id=\"info\" class=\"esri-widget\">\r\n            <v-btn elevation=\"2\" @click=\"addRoad()\" id=\"addBtn\">{{addButton}}</v-btn>\r\n             <div>\r\n                <h2>County Mileage</h2>\r\n                <p v-cloak>County: {{county}} / User Name: {{username}}</p>\r\n                <p v-cloak>Previous Total Mileage: {{countyTotal}}</p>\r\n                <p v-cloak>Current Mileage: {{currentMiles}}</p>\r\n                <p v-if=\"isNaN(countyTots) ? 0: countyTots\">New Total Miles: {{countyTots}}</p>\r\n            </div>\r\n        </div>\r\n     </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {addRoadbed, updateLength,modifyRoadbed, zoomExtents, hightlightFeat} from '../Map/editFunc'\r\nimport mileageReport from './mileageReport.vue'\r\n\r\n//import { gLayer } from '../Map/map';\r\n//import Graphic from \"@arcgis/core/Graphic\";\r\n//import * as geometryEngine from \"@arcgis/core/geometry/geometryEngine\";\r\n\r\nexport default {\r\n    name: 'Map',\r\n    components: {mileageReport\r\n    },\r\n    props:{\r\n        addRoadbed: Function,\r\n        addLen: Function\r\n    },\r\n    data(){\r\n        return{\r\n            addButton: \"add Road\",\r\n            previousTotal: 0,\r\n            county: \"Travis\",\r\n            username: 'DPROSACK',\r\n            countyTotal:120999,\r\n            lineLength: {},\r\n            newMiles: '',\r\n            modifyLine: 0,\r\n            modifyLength: 0\r\n        }\r\n    },\r\n    async mounted() {\r\n        const app = await import('../Map/map');\r\n        app.initialize(this.$el); \r\n        zoomExtents();\r\n        hightlightFeat();\r\n        \r\n    },\r\n    methods: {\r\n        addRoad() {\r\n            addRoadbed().then(result=>{\r\n            this.previousTotal += parseFloat(result.toFixed(3))\r\n            })\r\n        },\r\n    },\r\n\r\n    watch:{\r\n       previousTotal() {\r\n            addRoadbed().then(result=>\r\n            this.previousTotal += parseFloat(result.toFixed(3))\r\n            )\r\n        },\r\n\r\n        modifyLine:{\r\n             handler: function(){\r\n                modifyRoadbed().then(result => \r\n                this.modifyLine += parseFloat(result.toFixed(3)))\r\n            },\r\n            immediate: true,\r\n        },\r\n        \r\n       modifyLength:{\r\n            handler: function(){\r\n                updateLength().then(result => \r\n                this.modifyLength += parseFloat(result.toFixed(3)))\r\n            },\r\n            immediate: true, \r\n        },\r\n    },\r\n\r\n    computed:{\r\n        countyTots: function(){\r\n            return this.countyTotal + this.previousTotal + this.modifyLength + this.modifyLine\r\n        },\r\n\r\n        currentMiles: function(){\r\n            return this.previousTotal + this.modifyLength + this.modifyLine\r\n        }\r\n    }\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n#viewDiv {\r\n    position: absolute;\r\n    top: 0px;\r\n    right: 0;\r\n    height: 100%;\r\n    width: 100%;\r\n    z-index: 1;\r\n  }\r\n\r\n  .main{\r\n  position: absolute;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n  #info{\r\n  position: absolute;\r\n  padding: 10px;\r\n  top: 10%; \r\n  left: 80%;\r\n  width: 300px;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/components/Map"}]}